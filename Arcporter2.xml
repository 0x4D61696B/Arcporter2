                                        <RadioButton id="radio_challenge" dimensions="center-y:50%; left:250; height:16; width:16" key="DIFFICULTY_CHALLENGE" style="font:Demi_10;"/>
                                        <RadioButton id="radio_hard" dimensions="center-y:50%; left:100; height:16; width:16" key="DIFFICULTY_HARD" style="font:Demi_10;"/>
                                    </ListLayout>
                                </RadioGroup>

                                <!-- Instance / Zone Listing -->
                                <Group id="listing" dimensions="left:0; top:102; bottom:100%-2; width:465"/>
                                <!-- Instance / Zone Listing -->


                                <StillArt id="hardcore_background" dimensions="bottom:100%; right:100%; height:562; width:360;" style="texture:arcporter; region:hardcore_bg; alpha:0" />
                                <Group dimensions="right:100%-2; top:0; bottom:100%-2; width:360" >
                                    <Group id="queue_detail" dimensions="dock:fill" style="alpha:0" >
                                        <Text id="queue_label" key="{Words Words Words Words A Long Word, yo!}" dimensions="top:0; left:0; width:100%; height:85" style="font:UbuntuMedium_13; halign:center; valign:bottom; color:orange; wrap:true" />

                                        <!-- Thumbnail Large -->
                                        <Group dimensions="center-x:50%; top:102; width:296; aspect:1.8654">
                                            <Group id="detailscreenshot" dimensions="left:1; right:100%-1; top:1; bottom:100%-1" />
                                            <Border dimensions="dock:fill" class="ButtonBorder" style="alpha:0.15; exposure:1.0; padding:2"/>
                                        </Group>
                                        <!-- Thumbnail Large -->

                                        <!-- Selectable Thumbnails -->
                                        <Group id="thumbnails" dimensions="center-x:50%; top:263; height:54; width:296;" />
                                        <!-- Selectable Thumbnails -->

                                        <Group dimensions="left:0; right:100%; bottom:100%-243; height:1" blueprint="seperator" style="alpha:0.3" />

                                        <!-- Additional Lore -->
                                        <Group dimensions="dock:fill" style="visible:false">
                                            <Text id="detail_text" dimensions="left:0; top:0; bottom:100%; width:100%;" style="font:UbuntuRegular_9; halign:left; valign:top; wrap:true;"/>
                                        </Group>
                                        <Group dimensions="left:10%; right:90%; top:100%-243; bottom:100%-111;">
                                            <Group id="detail_scroller"  dimensions="left:5; right:100%-5; top:5; bottom:100%-5" />
                                        </Group>
                                        <!-- Additional Lore -->

