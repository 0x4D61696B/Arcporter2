                                    <Text dimensions="left:0; top:42; height:11; width:100%" key="INSTANCE_AVAILABILTY" style="font:Demi_11; halign:center; valign:bottom; visible:true"/>
                                </Group>
                                <RadioGroup id="diff_radio_group" dimensions="left:20; width:200; height:16; top:75;">
                                    <ListLayout dimensions="dock:fill" style="horizontal:true; hpadding:15;">
