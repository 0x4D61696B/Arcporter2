    <BluePrints>
        <BluePrint name="seperator" instantiable="true">
            <StillArt dimensions="top:0; bottom:100%; left:0; right:50%" style="texture:gradients; region:white_right;"/>
            <StillArt dimensions="top:0; bottom:100%; left:50%; right:100%" style="texture:gradients; region:white_left;"/>
        </BluePrint>

        <BluePrint name="instanceplate" instantiable="true">
            <StillArt dimensions="dock:fill" style="texture:colors; region:white; tint:#000000; alpha:0.9" />
            <StillArt name="typebar" dimensions="left:0; right:8; top:0; bottom:100%;" style="texture:colors; region:white;" />
            <Border name="highlight" dimensions="left:9; right:100%; top:0; bottom:100%;" class="Highlight" style="padding:48; alpha:0" />
            <Group name="thumbnail" dimensions="left:16; top:6; height:80; width:85">
                <Border dimensions="left:0; top:0; height:79; width:79;" class="RoundedBorders" style="padding:3; tint:#000000; alpha:1;" />
                <Group name="icon" dimensions="left:2; top:2; height:73; width:73"/>
                <Group name="queued" dimensions="left:2; bottom:100%-5; height:20; width:73;" style="alpha:0" >
                    <StillArt dimensions="dock:fill" style="texture:colors; region:white; tint:#00FF00; alpha:0.5" />
                    <Text key="QUEUED" dimensions="dock:fill" class="font:UbuntuMedium_10" />
                </Group>
                <StillArt name="check" dimensions="top:0; right:100%; height:23; width:23;" style="texture:icons_arcporter; region:check; alpha:0" />
            </Group>
            <ListLayout name="info" dimensions="left:105; right:100%; top:6; bottom:100%;" style="vpadding:2">
                <Text name="name" key="QUEUE_NAME" dimensions="top:0; left:0; width:100%; height:18" class="plate_header" />
                <Text name="subtext" dimensions="top:0; left:0; width:100%; height:18" class="plate_detail" />
                <ListLayout name="detail" dimensions="top:0; left:0; width:100%; height:18" style="horizontal:true; hpadding:5">
                    <Text name="req" key="Required" dimensions="top:0; left:0; width:60; height:100%" class="plate_grey" />
                    <Group name="level_min" dimensions="left:0; top:0; height:100%; width:50" >
                        <Border dimensions="dock:fill" class="PanelSubBackDrop" style="padding:3; tint:#1F2022; alpha:1"/>
                        <Text name="label" dimensions="top:0; left:0; width:100%; height:100%" class="plate_center_small" />
                    </Group>
                    <Group name="player_count" dimensions="left:0; top:0; height:100%; width:115" >
                        <Border dimensions="dock:fill" class="PanelSubBackDrop" style="padding:3; tint:#1F2022; alpha:1"/>
                        <Text name="label" dimensions="top:0; left:0; width:100%; height:100%" class="plate_center_small" />
                    </Group>
                </ListLayout>
                <ListLayout name="sov_detail" dimensions="top:0; left:0; width:100%; height:18" style="horizontal:true; hpadding:5; visible:false">
                    <Text name="sov" key="SOVEREIGNTY" dimensions="top:0; left:0; width:80; height:100%" class="plate_grey" />
                    <Group name="sov_owner" dimensions="left:0; top:0; height:100%; width:50" >
                        <Border dimensions="dock:fill" class="PanelSubBackDrop" style="padding:3; tint:#1F2022; alpha:1"/>
                        <Text name="label" dimensions="top:0; left:0; width:100%; height:100%" class="plate_center_small" />
                    </Group>
                </ListLayout>
                <ListLayout name="daily_completion" dimensions="top:0; left:0; width:100%; height:18" style="horizontal:true; hpadding:2; visible:false">
                    <Group dimensions="top:0; left:0; width:18; height:100%;">
                        <StillArt dimensions="right:100%; center-y:50%; width:13; height:10" style="texture:check_icon;"/>
                    </Group>
                    <Text key="DAILY_COMPLETED_TODAY" dimensions="top:0; left:0; width:100%; height:100%" class="plate_grey"/>
                </ListLayout>
            </ListLayout>

            <FocusBox name="FocusBox" dimensions="dock:fill" class="ui_button" />
        </BluePrint>
        <BluePrint name="req_tooltip" instantiable="true">
            <ListLayout name="reason_list" dimensions="top:0; left:0; height:100%; width:100%" style="hpadding:8; vpadding:0">
                <Group name="hardcore_req" dimensions="top:0; left:0; width:100%; height:20">
                    <Text name="reason" dimensions="dock:fill" key="ARC_REQ_NO_HARDCORE" class="req_reason"/>
                </Group>
                <Group name="challenge_req" dimensions="top:0; left:0; width:100%; height:20">
                    <Text name="reason" dimensions="dock:fill" key="ARC_REQ_NO_CHALLENGE" class="req_reason"/>
                </Group>
                <Group name="group_req" dimensions="top:0; left:0; width:100%; height:20">
                    <Text name="reason" dimensions="dock:fill" key="ARC_REQ_GROUP_SIZE" class="req_reason"/>
                </Group>
                <ListLayout name="level_req" dimensions="top:0; left:0; width:10; height:40" style="vpadding:6; hpadding:6">
                    <Text name="reason" dimensions="top:0; left:0; width:100%; height:20" key="ARC_REQ_LEVEL" class="req_reason"/>
                    <Text name="names" dimensions="top:0; left:0; width:100%; height:20" class="req_names" key="{PlayerName, PlayerName, PlayerName, PlayerName}"/>
                </ListLayout>
                <ListLayout name="cert_req" dimensions="top:0; left:0; width:10; height:40" style="vpadding:6; hpadding:6">
                    <Text name="reason" dimensions="top:0; left:0; width:100%; height:20" key="ARC_REQ_CERT" class="req_reason"/>
                    <Text name="names" dimensions="top:0; left:0; width:100%; height:20" class="req_names" key="{PlayerName, PlayerName, PlayerName, PlayerName, PlayerName}"/>
                </ListLayout>
            </ListLayout>
        </BluePrint>
    </BluePrints>

    <Frames>
        <WindowFrame name="main" dimensions="center-x:50%; center-y:50%; height:650; width:829" visible="false" depth="0" key="ARCPORTER" style="movable:true"><!-- PanelLayer3 -->
            <Children>
                <Group id="window" dimensions="dock:fill" style="alpha:1">
                    <!-- Body -->
                    <Group dimensions="left:0; top:0; bottom:100%; right:100%" >
                        <!-- Fosterable Group -->
                        <Group id="queue_page" dimensions="dock:fill">
                            <!-- Header & Tabs -->
                            <Group dimensions="left:1; right:100%-1; top:1; height:45">
                                <Group id="tabs" dimensions="dock:fill" />
                            </Group>
                            <Text id="choose_label" key="CHOOSE_INSTANCE" dimensions="left:25; top:1; width:100%-25; height:65" style="font:Demi_25; halign:left; valign:center; visible:false"/>
                            <!-- Header & Tabs -->

                            <!-- Body -->
                            <Group dimensions="left:1; right:100%-1; top:23; bottom:100%-1;" >
